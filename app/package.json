    "@testing-library/vue": "^6.6.1",
    "@types/bcrypt": "^5.0.2",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.26.0",
    "@vitest/ui": "^0.26.1",
    "dayjs": "^1.11.7",
    "nuxt": "^3.8.0",
    "playwright": "^1.29.2",
    "prisma": "^4.8.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.0.16"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.1.58",
